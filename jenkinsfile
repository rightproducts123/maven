@Library("mylibrary")_
pipeline
{
    agent any
    stages
    {
        stage('contDownload_Loans')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("maven")
                }
            }
        }
        stage('contBuild')
        {
            steps
            {
                script
                {
                    cicd.mavenBuild()
                }
            }
        }
      }
}      
